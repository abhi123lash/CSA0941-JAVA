import java.util.*;
public class Collect {
    public static void main(String args[]) {
        LinkedList<String> al = new LinkedList<String>();
        al.add("Apple");
        al.add("Mango");
        al.add("Orange");
        al.add("Banana");
        al.remove("Apple");
        Iterator<String> itr = al.iterator();
        while (itr.hasNext()) {
            System.out.println(itr.next());

        }
    }
}
