import java.util.*;
public class QueueExample {
    public static void main(String[] args) {
        Queue<String> al = new LinkedList<String>();
        al.add("Apple");
        al.add("Mango");
        al.add("Banana");
        al.add("Orange");
        al.remove("Banana");
        Iterator<String> itr = al.iterator();
        while (itr.hasNext()) {
            System.out.println(itr.next());
        }
    }
}
