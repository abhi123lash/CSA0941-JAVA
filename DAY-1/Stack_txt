import java.util.*;
public class StackExample {
    public static void main(String[] args) {
        Stack<String> al = new Stack<String>();
        al.add("Apple");
        al.add("Mango");
        al.add("Banana");
        al.add("Orange");
        al.remove("Banana");
        Iterator<String> itr =al.iterator();
        while (itr.hasNext()) {
            System.out.println(itr.next());
        }

    }
}
